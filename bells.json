{
  "Bells": {
    "homeroom": {
      "Enabled": false,
      "Name": "Homeroom Bell",
      "Description": "Plays a bell at the end of homeroom",
      "Locked": "false",
      "Time": "11 9 * * *",
      "File": "bell.mp3",
      "TriggerEmail": {
        "Enabled": true,
        "From": "graydav1@lavalla.vic.edu.au",
        "To": "graydav1@lavalla.vic.edu.au",
        "Body": "It was triggered on <%= Date %>",
        "Subject": "<%= item.Name %> has been triggered!"
      },
      "ChangeEmail": {
        "Enabled": true,
        "From": "graydav1@lavalla.vic.edu.au",
        "To": "graydav1@lavalla.vic.edu.au",
        "Body": "<%= item.Name %> has been changed to <%= item.Enabled %>",
        "Subject": "It was changed on <%= Date %>. If this shouldn't happen, please enable it again"
      }
    },
    "recess": {
      "Enabled": true,
      "Name": "Recess Bell",
      "Description": "Plays a bell at recess.",
      "Locked": "false",
      "Time": "* * * * *",
      "File": "bell.mp3",
      "TriggerEmail": {
        "Enabled": "true",
        "From": "graydav1@lavalla.vic.edu.au",
        "To": "graydav1@lavalla.vic.edu.au",
        "Body": "It was triggered on <%= Date %>",
        "Subject": "<%= item.Name %> has been triggered"
      },
      "ChangeEmail": {
        "Enabled": true,
        "From": "graydav1@lavalla.vic.edu.au",
        "To": "graydav1@lavalla.vic.edu.au",
        "Body": "<%= item.Name %> has been changed to <%= item.Enabled %>",
        "Subject": "It was changed on <%= Date %>. If this shouldn't happen, please enable it again"
      }
    }
  }
}
